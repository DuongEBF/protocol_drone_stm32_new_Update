#ifndef MAVLINK_FROM_FC_H_
#define MAVLINK_FROM_FC_H_
#include "mavlink.h"
void mavlink_init(void);
typedef struct
{
	mavlink_message_t rx_msg;
	mavlink_status_t status;
	mavlink_heartbeat_t hb;
}mavlink_from_fc_t;


#endif /* LED_CONTROLL_MAVLINK_FROM_FC_H_ */
